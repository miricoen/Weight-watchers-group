{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SubscriberData": "Data Source=DESKTOP-PL68J44;Initial Catalog=SubscriberDatabase;Integrated Security=true; User ID=miri;Password=1234; TrustServerCertificate=True; Encrypt=False;"

  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "setting": {
    "FileSave": "C:\\logs",
    "AdministratorId": 1,
    "Jwt": {
      "Issuer": "https://localhost:7074",
      "Audience": "https://localhost:7074",
      "Key": "This is a sample secret key - please don't use in production environment."
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "WriteTo": [
    {
      "Name": "Console"
    },
    {
      "Name": "File",
      "Args": {
        "path": "c://logs//log-.txt",
        "rollOnFileSizeLimit": true,
        "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter,Serilog.Formatting.Compact",
        "rollingInterval": "Day"
      }
    }
  ],
  "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ]


}
